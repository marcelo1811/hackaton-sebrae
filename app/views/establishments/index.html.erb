 <!doctype html>
<html lang="en">



<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="./css/bootstrap.min.css">
    <title>Data analytics</title>
</head>

<style type="text/css">
    body {
        font-family: Helvetica !important;
    }

    .btn-circle {
        width: 30px;
        height: 30px;
        padding: 6px 0px;
        border-radius: 15px;
        text-align: center;
        font-size: 12px;
        line-height: 1.42857;
        background-color: #1b7abb;
        color: white;
    }

    .btn-icon {
        line-height: 0.7;
    }

    .header {
        background-color: #1b7abb;
        color: white;
        padding: 0;
    }

    .logo {
        width: 6%;
        margin-left: 120px;
    }

    .header-text {
        text-align: center;
        width: 100%;
        position: absolute;
    }

    .content-grouper {
        margin: 0 30px 0 30px;
    }

    .content-table {
        margin: 12px 0 12px 0;
    }

    .input-division {
        width: calc(20% - 15px);
        margin-right: 15px;
    }

    .user-icon {
        display: block;
        float: right;
        margin-right: 30px;
    }

    .ze-big-balls {
        width: 40px;
        height: 40px;
        border-radius: 40px;
    }

    .icon-of-big-balls {
        font-size: 30px;
        margin-top: 2px;
    }

    .search-button {
        background-color: #CB6339;
    }


    td > .btn-circle {
        background-color: transparent;
        border: none;
    }

    td > .btn-circle > i {
        color: black;
    }
</style>

<body>
    <div class="content-grouper">
        <div class="row" style="margin: 0">
            <div class="input-division">
                Nome
            </div>
            <div class="input-division">
                Cidade
            </div>
            <div class="input-division">
                Bairro
            </div>
            <div class="input-division">
                E-mail
            </div>
            <div class="input-division">
                Nível
            </div>
        </div>
        <div class="row content-table">

            <input type="text" class="form-control input-division" id="name-filter" placeholder="Filtro de nome" />
            <input type="text" class="form-control input-division" id="city-filter" placeholder="Filtro de cidade" />
            <input type="text" class="form-control input-division" id="neighborhood-filter"
                placeholder="Filtro de bairro" />
            <input type="text" class="form-control input-division" id="email-filter" placeholder="Filtro de e-mail" />
            <div class="dropdown" style="width: 12%" id="level-filter">
                <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton"
                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Filtro de nível
                </button>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <a class="dropdown-item" href="#">1</a>
                    <a class="dropdown-item" href="#">2</a>
                    <a class="dropdown-item" href="#">3</a>
                    <a class="dropdown-item" href="#">4</a>
                    <a class="dropdown-item" href="#">5</a>
                </div>
            </div>
            <button type="button" class="btn btn-circle btn-lg ze-big-balls search-button" style="margin-right: 15px;" data-toggle="tooltip"
                data-placement="top" title="Pesquisar">
                <i class="material-icons btn-icon icon-of-big-balls">search</i>
            </button>
            <button type="button" class="btn btn-circle btn-lg ze-big-balls" data-toggle="tooltip" data-placement="top"
                title="Adicionar Estabelecimento">
                <i class="material-icons btn-icon icon-of-big-balls">add</i>
            </button>
        </div>


        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">CNPJ</th>
                    <th scope="col">Nome Fantasia</th>
                    <th scope="col">Tipo</th>
                    <th scope="col">Rua</th>
                    <th scope="col">Número</th>
                    <th scope="col">Bairro</th>
                    <th scope="col">Cidade</th>
                    <th scope="col">Fone</th>
                    <th scope="col">E-mail</th>
                    <th scope="col">Whatsapp</th>
                    <th scope="col">Nível</th>
                    <th scope="col">Ações</th>
                </tr>
            </thead>
            <tbody>
              <%= render partial: "establishment", collection: @establishments %>
            </tbody>
        </table>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="./js/bootstrap.min.js"></script>
</body>
<script>
    $('[data-toggle="tooltip"]').tooltip()
</script>

</html>
