<tr>
  <td><%= establishment.cpf_cnpj %></td>
  <td><%= establishment_fantasy_name(establishment) %></td>
  <td>Pizzaria</td>
  <td><%= establishment_address_street(establishment) %></td>
  <td><%= establishment_address_number(establishment) %></td>
  <td><%= establishment_address_neighborhood(establishment) %></td>
  <td><%= establishment_address_city(establishment) %></td>
  <td><%= establishment_phone_number(establishment) %></td>
  <td><%= establishment_email(establishment) %></td>
  <td><%= establishment_whatsapp(establishment) %></td>
  <td>
    <i class="material-icons">stars</i>
    <i class="material-icons">stars</i>
    <i class="material-icons">stars</i>
  </td>
  <td>
    <%= link_to establishment_path(establishment) do %>
      <button type="button" class="btn btn-primary btn-circle" data-toggle="tooltip" data-placement="top"
      title="Histórico">
        <i class="material-icons btn-icon">history</i>
      </button>
    <% end %>
    <%= link_to edit_establishment_path(establishment) do %>
      <button type="button" class="btn btn-primary btn-circle" data-toggle="tooltip" data-placement="top"
      title="Observações">
        <i class="material-icons btn-icon">comment</i>
      </button>
    <% end %>
    <%= link_to edit_establishment_path(establishment) do %>
      <button type="button" class="btn btn-primary btn-circle" data-toggle="tooltip" data-placement="top"
      title="Editar">
        <i class="material-icons btn-icon">edit</i>
      </button>
    <% end %>
  </td>
</tr>
